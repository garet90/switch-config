[Unit]
Description=Configures and activates the switch
After=opx-cps.service opx-pas.service opx-nas.service opx-platform-init.service
Wants=opx-cps.service opx-pas.service opx-nas.service opx-platform-init.service

[Service]
ExecStart=/usr/bin/switch-config --file /etc/switch-config/config.yaml

[Install]
WantedBy=multi-user.target
